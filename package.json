{
  "name": "vite-react-ts-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "npm run lint --fix & prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
